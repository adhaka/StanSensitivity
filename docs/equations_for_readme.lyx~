\begin_body

\begin_layout Standard
\begin_inset FormulaMacro
\newcommand{\mbe}{\mathbb{E}}
{\mathbb{E}}
\end_inset


\end_layout

\begin_layout Standard
\begin_inset FormulaMacro
\newcommand{\var}{\mathrm{Var}}
{\mathrm{Var}}
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\textrm{Parameters:}\quad & \theta\\
\textrm{Data:}\quad & x\\
\textrm{Hyperparameter:}\quad & \alpha
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\textrm{Prior:}\quad & p\left(\theta\vert\alpha\right)\\
\textrm{Likelihood:}\quad & p\left(x\vert\theta,\alpha\right)\\
\textrm{Posterior:}\quad & p\left(\theta\vert x,\alpha\right)=\frac{p\left(\theta\vert\alpha\right)p\left(x\vert\theta,\alpha\right)}{p\left(x\vert\alpha\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\textrm{Expectation of interest:}\quad & \mbe\left[g\left(\theta\right)\vert x,\alpha\right]\\
\textrm{Posterior variance:\ensuremath{\quad}} & \var\left(g\left(\theta\right)\vert x,\alpha\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\textrm{Sensitivity:}\quad & \left.\frac{d\mbe\left[g\left(\theta\right)\vert x,\alpha\right]}{d\alpha}\right|_{\alpha_{0}}\\
\textrm{Normalized sensitivity:\ensuremath{\quad}} & \frac{\left.d\mbe\left[g\left(\theta\right)\vert x,\alpha\right]/d\alpha\right|_{\alpha_{0}}}{\sqrt{\var\left(g\left(\theta\right)\vert x,\alpha\right)}}
\end{align*}

\end_inset


\end_layout

